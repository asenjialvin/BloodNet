{
  "expo": {
    "name": "bloodnet",
    "slug": "bloodnet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "bloodnet",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "sdk.dir": "/home/muhindi/Android/Sdk",
        "package": "com.muhindi.bloodnet",
        "googleMapsApiKey": "AIzaSyCzzzySEQVUzOXwH7Mb_J01MMWWJeIlFSI"
      }
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-location"],
    "configPlugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(BloodNet) to use your location."
        }
      ]
    ]
  }
}